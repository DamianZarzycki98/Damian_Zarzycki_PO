<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../../css/css.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.edit.WydawnictwoEdit">
   <children>
      <VBox fx:id="edit" prefHeight="400.0" prefWidth="220.0">
         <children>
            <ComboBox fx:id="selectWydawnictwo" onAction="#wydawnictwo" prefWidth="180.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin></ComboBox>
            <Label text="Nazwa wydawnictwa">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="wydawnictwoEditNazwa">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></TextField>
            <Label text="Siedziba główna">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="wydawnictwoEditSiedziba">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></TextField>
            <Label text="E-mail">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="wydawnictwoEditEmail">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></TextField>
            <Button mnemonicParsing="false" onAction="#wydawnictwoEdit" text="Edytuj">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
