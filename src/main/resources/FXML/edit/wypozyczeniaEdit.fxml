<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../../css/css.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.edit.WypozyczeniaEdit">
   <children>
      <VBox fx:id="edit" prefHeight="400.0" prefWidth="220.0">
         <children>
            <ComboBox fx:id="selectWypozyczenia" onAction="#wypozyczenia" prefWidth="180.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin></ComboBox>
            <Label text="Wybierz książkę">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <ComboBox fx:id="wypozyczeniaSelectKsiazka" prefWidth="180.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></ComboBox>
            <Label text="Data oddania">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Label>
            <DatePicker fx:id="wypozyczeniaSelectDataOdd" prefWidth="180.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></DatePicker>
            <Button mnemonicParsing="false" onAction="#wypozyczeniaEdit" text="Edytuj">
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
