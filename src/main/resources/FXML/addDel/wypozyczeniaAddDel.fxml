<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../../css/css.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.addDel.WypozyczeniaAddDel">
   <children>
      <TabPane fx:id="edit" prefHeight="400.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Dodawanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Wybierz książkę">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="wypozyczeniaSelectKsiazka" prefWidth="160.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label text="Wybierz czytelnika">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="wypozyczeniaSelectCzytelnik" prefWidth="160.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label text="Data oddania">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <DatePicker fx:id="wypozyczeniaSelectDataOdd" prefWidth="160.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </DatePicker>
                              <Button mnemonicParsing="false" onAction="#wypozyczeniaAdd" text="Dodaj">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Usuwanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Wybierz wypozyczenie do usunięcia">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="wypozyczeniaSelect" prefWidth="160.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Button mnemonicParsing="false" onAction="#wypozyczeniaDel" text="Usuń">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
