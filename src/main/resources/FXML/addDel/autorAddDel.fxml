<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" stylesheets="@../../css/css.css" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.addDel.AutorAddDel">
   <children>
      <TabPane fx:id="edit" prefHeight="400.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Dodawanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Imie">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="autorAddImie">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Label text="Nazwisko">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="autorAddNazwisko">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Label text="Rok urodzenia">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="autorAddRok">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Label text="Narodowość">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="autorAddNarodowosc">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Button mnemonicParsing="false" onAction="#autorAdd" text="Dodaj">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Usuwanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Wybierz autora do usunięcia">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="autorSelect" prefWidth="180.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Button mnemonicParsing="false" onAction="#autorDel" text="Usuń">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TabPane>
   </children>
</AnchorPane>
