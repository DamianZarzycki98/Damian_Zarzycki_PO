<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../../css/css.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.addDel.EgzemplarzAddDel">
   <children>
      <TabPane fx:id="edit" prefHeight="400.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Dodawanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Wybierz książkę" wrapText="true">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="egzemplarzSelectKsiazka" prefWidth="160.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label text="Podaj ilość książek do dodania">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="egzemplarzAddIlosc">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Button mnemonicParsing="false" onAction="#egzemplarzAdd" text="Dodaj">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Usuwanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefWidth="200.0">
                           <children>
                              <Label text="Wybierz książkę">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="egzemplarzDelSelect" prefWidth="150.0">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label text="Podaj ilość książek do usunięcia">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                              <TextField fx:id="egzemplarzDelIlosc">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </TextField>
                              <Button mnemonicParsing="false" onAction="#egzemplarzDel" text="Usuń">
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
