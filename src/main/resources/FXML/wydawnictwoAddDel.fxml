<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cfg.WydawnictwoAddDel">
   <children>
      <TabPane prefHeight="450.0" prefWidth="400.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Dodawanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="wydawnictwoAddNazwa" layoutX="51.0" layoutY="47.0" />
                        <TextField fx:id="wydawnictwoAddSiedziba" layoutX="51.0" layoutY="123.0" />
                        <TextField fx:id="wydawnictwoAddEmail" layoutX="51.0" layoutY="198.0" />
                        <Text layoutX="51.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nazwa wydawnictwa" />
                        <Text layoutX="51.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Siedziba główna" />
                        <Text layoutX="51.0" layoutY="194.0" strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail" />
                        <Button layoutX="51.0" layoutY="245.0" mnemonicParsing="false" onAction="#wydawnictwoAdd" text="Dodaj" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Usuwanie">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="wydawnictwoSelect" layoutX="50.0" layoutY="53.0" prefWidth="150.0" />
                        <Text layoutX="50.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wybierz wydawnictwo do usunięcia" />
                        <Button layoutX="50.0" layoutY="100.0" mnemonicParsing="false" onAction="#wydawnictwoDel" text="Usuń" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
